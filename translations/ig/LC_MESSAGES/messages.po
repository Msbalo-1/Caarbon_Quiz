msgstr "Ịrụ ọrụ na ọgwụgwọ kemịkalụ"

msgid "Ugbo nke na-achọkarị mmiri"
msgstr "Ugbo nke na-achọkarị mmiri"

msgid ""
"Agroforestry bụ ịkekọrịta osisi na usoro ugbo, nke na-enyere aka ịwụnye "
"carbon dioxide, belata mmerụ ala, na ịnọgide na-enwe ọdịiche nke ndụ."
msgstr ""
"Agroforestry bụ ịkekọrịta osisi na usoro ugbo, nke na-enyere aka ịwụnye "
"carbon dioxide, belata mmerụ ala, na ịnọgide na-enwe ọdịiche nke ndụ."

msgid "Gịnị bụ mmetụta gburugburu ebe obibi nke njem nri na itufu carbon?"
msgstr "Ọ na-abawanye itufu carbon n'ihi iji mmanụ"

msgid "Ọ na-abawanye itufu carbon n'ihi iji mmanụ"
msgstr "Ọ na-abawanye itufu carbon n'ihi iji mmanụ"

msgid "Ọ na-ebelata itufu carbon site n'iji ụgbọ ala eletrik"
msgstr "Ọ na-ebelata itufu carbon site n'iji ụgbọ ala eletrik"

msgid "Ọ naghị enwe mmetụta na itufu carbon"
msgstr "Ọ naghị enwe mmetụta na itufu carbon"

msgid "Ọ na-enyere ị nweta itufu carbon site na akpa nri"
msgstr "Ọ na-enyere ị nweta itufu carbon site na akpa nri"

msgid ""
"Ije ozi nri chọrọ mmanụ, nke na-enyere aka itufu carbon site na ọkụ nke "
"mmanụ fossil."
msgstr ""
"Ije ozi nri chọrọ mmanụ, nke na-enyere aka itufu carbon site na ọkụ nke "
"mmanụ fossil."

msgid "Olee otú nkụcha nri si na-enyere aka na itufu carbon?"
msgstr "Nkwụcha nri na-emepụta methane na ebe mkpofu"

msgid "Nkwụcha nri na-emepụta methane na ebe mkpofu"
msgstr "Nkwụcha nri na-emepụta methane na ebe mkpofu"

msgid "Nkwụcha nri na-ebelata itufu carbon"
msgstr "Nkwụcha nri na-ebelata itufu carbon"

msgid "Nkwụcha nri anaghị enwe mmetụta na itufu carbon"
msgstr "Nkwụcha nri anaghị enwe mmetụta na itufu carbon"

msgid "Nkwụcha nri bụ ihe nwere ike idozi ma ọ naghị emepụta gas"
msgstr "Nkwụcha nri bụ ihe nwere ike idozi ma ọ naghị emepụta gas"

msgid ""
"Mgbe nkụcha nri na-emepe na ebe mkpofu, ọ na-emepụta methane, gas na-akpali "
"ọkụ nke na-enyere aka na mgbanwe ihu igwe."
msgstr ""
"Mgbe nkụcha nri na-emepe na ebe mkpofu, ọ na-emepụta methane, gas na-akpali "
"ọkụ nke na-enyere aka na mgbanwe ihu igwe."

msgid "Kedu nri nke a nwere carbon footprint kacha nta?"
msgstr "Nri sitere na osisi"

msgid "Nri sitere na osisi"
msgstr "Nri sitere na osisi"

msgid "Nri nwere anụ"
msgstr "Nri nwere anụ"

msgid "Nri nwere nri emere"
msgstr "Nri nwere nri emere"

msgid "Nri nchịkọta nwere ọtụtụ ngwaahịa dairy"
msgstr "Nri nchịkọta nwere ọtụtụ ngwaahịa dairy"

msgid ""
"Nri sitere na osisi na-adịkarị na carbon footprint nke nta n'ihi na ha chọrọ"
" obere ihe, gụnyere mmiri na ike, na na-emepụta obere gas na-emepụta "
"okpomọkụ ụwa."
msgstr ""
"Nri sitere na osisi na-adịkarị na carbon footprint nke nta n'ihi na ha chọrọ"
" obere ihe, gụnyere mmiri na ike, na na-emepụta obere gas na-emepụta "
"okpomọkụ ụwa."

msgid "Kedu mmetụta carbon nke ịkụ nri na gburugburu ebe obibi?"
msgstr "Ngwaahịa eji eme ngwugwu na-eji plastik, nke na-eme ka mmetọ plastik"

msgid "Ngwaahịa eji eme ngwugwu na-eji plastik, nke na-eme ka mmetọ plastik"
msgstr "Ngwaahịa eji eme ngwugwu na-eji plastik, nke na-eme ka mmetọ plastik"

msgid "Ngwugwu na-ebelata nkụcha nri"
msgstr "Ngwugwu na-ebelata nkụcha nri"

msgid "Ngwugwu anaghị enwe mmetụta na itufu carbon"
msgstr "Ngwugwu anaghị enwe mmetụta na itufu carbon"

msgid "Ngwugwu na-enyere nchekwa nri maka oge ogologo"
msgstr "Ngwugwu na-enyere nchekwa nri maka oge ogologo"

msgid ""
"Ngwugwu nri na-eji plastik, nke na-eme ka mmetọ plastik na ụkọ nke ihe, ma "
